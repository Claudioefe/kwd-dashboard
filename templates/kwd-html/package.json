  "keywords": [
    "dashboard",
    "tailwindcss",
    "layout",
    "alpinejs",
    "admin",
    "template"
  ],
  "scripts": {
    "dev": "run-p js:watch html:watch vendor css:watch serve",
    "build": "run-s js:build html:build css:build vendor",
    "serve": "node scripts/cli serve -w",
    "vendor": "gulp",
    "html:build": "node scripts/cli html",
    "html:watch": "node scripts/cli html -w",
    "js:build": "node scripts/cli js",
    "js:watch": "node scripts/cli js -w",
    "css:build": "tailwind -i src/assets/css/tailwind.css -o dist/assets/css/main.css -m",
    "css:watch": "tailwind -i src/assets/css/tailwind.css -o dist/assets/css/main.css -w"
  },
  "devDependencies": {
    "alpinejs": "^3.2.4",
    "autoprefixer": "^10.3.1",
    "boxicons": "^2.0.9",
    "browser-sync": "^2.27.5",
    "chalk": "^4.1.2",
    "commander": "^8.1.0",
    "esbuild": "^0.12.24",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "liquidjs": "^9.25.1",
    "normalize-path": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "tailwindcss": "^2.2.7"
  }
}
